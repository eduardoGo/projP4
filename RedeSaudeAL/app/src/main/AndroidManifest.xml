<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.redesaudeal.app.redesaude">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GUI.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GUI.RegisterScreen" />
        <activity android:name=".GUI.AdminScreen" />
        <activity android:name=".GUI.RegisterAgentScreen" />
        <activity android:name=".GUI.RegisterAssistedPersonScreen" />
        <activity android:name=".GUI.RegisterFamily" />
        <activity android:name=".GUI.RegisterFamilyScreen" />
        <activity android:name=".GUI.HealthAgentScreen" />
        <activity android:name=".GUI.PerfilScreen" />
    </application>

</manifest>